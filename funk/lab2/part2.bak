(display "Клим Липовський ІПЗ-41. Лаб 1. Варіант 9. Частина 2.\n")

(define (f n)
  (cond ((= (/ n 2) 1) (display "Yes"))
        ((EVEN? n) (f (/ n 2)))  ; Перевірка чи n парне
        ((ODD? n) (display "No")) ; Перевірка чи n непарне
        )
  )

(display "Перевірка чи 20 є степенем двійки: ") (f 20)
(display "\n")
(display "Перевірка чи 16 є степенем двійки: ") (f 16)